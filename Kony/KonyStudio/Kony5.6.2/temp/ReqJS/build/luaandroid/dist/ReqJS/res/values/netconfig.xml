<resources>
	<!-- Enable/Disable connection pooling feature. Supported values are true and false --> 
	<item type="net" name="enable_conn_pool">true</item>
	<!-- Total number of connections in pool --> 
	<item type="net" name="conn_total">20</item>
	<!-- Connection per route in pool --> 
	<item type="net" name="conn_per_route">10</item>
	<item type="net" name="enable_gzip">true</item>
	<item type="net" name="expect_100_continue">true</item>
</resources>
